server {

    server_name sikforsbygg.se www.sikforsbygg.se;

    location / {
        proxy_pass http://10.0.0.192:8000;
	    proxy_set_header Host $host;
    	proxy_set_header X-Real-IP $remote_addr;
    }

    listen [::]:443 ssl;
    listen 443 ssl;
    ssl_certificate /root/sitecerts/sikforsbygg.crt;
    ssl_certificate_key /root/sitecerts/sikforsbygg.key;
}

server {

    server_name sikforsbygg.se www.sikforsbygg.se;

    return 301 https://$host$request_uri;
}

server {
    server_name www.algodoo.com algodoo.com;
    listen 80;
    listen [::]:80;

    location / {
        root /var/www/html/algodoo;
    }
}